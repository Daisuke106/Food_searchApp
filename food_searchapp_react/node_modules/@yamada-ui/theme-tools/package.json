{
  "name": "@yamada-ui/theme-tools",
  "version": "1.0.32",
  "description": "Yamada UI theme utils",
  "keywords": [
    "yamada",
    "yamada ui",
    "theme",
    "theming",
    "ui",
    "utils",
    "utilities"
  ],
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://yamada-ui.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yamada-ui/yamada-ui",
    "directory": "packages/theme-tools"
  },
  "bugs": {
    "url": "https://github.com/yamada-ui/yamada-ui/issues"
  },
  "devDependencies": {
    "clean-package": "2.2.0"
  },
  "dependencies": {
    "@yamada-ui/core": "1.6.7",
    "@yamada-ui/theme": "1.7.0",
    "@yamada-ui/utils": "1.2.0"
  },
  "clean-package": "../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ],
    "sourcemap": true
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "pnpm build:fast -- --watch",
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit"
  }
}